package lab3Package;

public class Debug {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum=0, n;
		System.out.println("sum before loop is"+sum);
		for(n=1;n<10;n++){
			sum=sum+n;
			System.out.println("n is "+n+" sum so far is "+sum);
		}
		System.out.println("1+2+...+9+10=="+sum);
	}

}
